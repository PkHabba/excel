{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "space-before-blocks": "off",
    "indent": "off",
    "no-empty": "off",
    "no-undef": "off",
    "no-invalid-this": "off",
    "no-trailing-spaces": "off",
    "no-unused-vars": "off",
    "linebreak-style": 0
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}